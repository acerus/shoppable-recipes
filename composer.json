{
  "name": "whisk/whisk-widgets",
  "description": "Whisk widgets for recipes: Shopping List",
  "license": "GPL-2.0",
  "keywords": [
    "whisk, shoppable recipes, shopping list, widget, foodtech"
  ],
  "homepage": "https://github.com/mihdan/whisk-recipe-widgets",
  "type": "wordpress-plugin",
  "authors": [
    {
      "name": "Pavel Fedorov",
      "email": "mikhail@kobzarev.com",
      "homepage": "https://www.kobzarev.com/"
    }
  ],
  "support": {
    "issues": "https://github.com/mihdan/whisk-recipe-widgets/issues"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "require": {
    "composer/installers": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "sebastian/phpcpd": "^3.0",
    "10up/wp_mock": "~0.3",
    "squizlabs/php_codesniffer": "^2.9",
    "phpcompatibility/php-compatibility": "^9.0",
    "wp-coding-standards/wpcs": "^1.2",
    "wp-cli/wp-cli": "*"
  },
  "autoload": {
    "classmap": [
      "includes"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "includes"
    ]
  }
}
